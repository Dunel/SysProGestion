generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Coderegister {
  id        String   @id @default(cuid())
  code      Int
  mail      String   @unique
  updatedAt DateTime @updatedAt
}
